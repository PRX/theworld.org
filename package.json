    "isomorphic-unfetch": "^3.0.0",
    "jquery": "^3.4.1",
    "jsonapi-relate": "^2.0.0",
    "lodash": "4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "next": "^9.1.5",
