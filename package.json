{
  "name": "theworldorg",
  "private": true,
  "description": "The World website fronetend.",
  "repository": "git@github.com:PRX/theworld.org.git",
  "author": "PRX",
  "contributors": [
    {
      "name": "Rick Peterman",
      "email": "rick.peterman@prx.org"
    }
